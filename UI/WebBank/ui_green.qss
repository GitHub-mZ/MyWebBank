*
{
	border-radius:0;
	font-family:"Noto Sans CJK KR Regular";
	font-size:18px;
	text-align:left;
}

/*Login*/
QLabel#LogInBG
{
        background-image:url(:/image/image/green/login.png);
        background-repeat:no-repeat;
}

QLabel#RegBG
{
        background-image:url(:/image/image/green/reg.png);
}

QLabel#LogInNotice
{
	color:red;
	font:16px;
	text-align:center;
}

QLabel#LoadingLabel{
        background-image:url(:/image/image/loading.png);
}

/*MsgBox*/
QLabel#MsgBoxBG
{
        background-image:url(:/image/image/green/msgbox.png);
}

QLabel#AboutBoxBG
{
        background-image:url(:/image/image/green/aboutbox.png);
}

QLabel#MsgBoxContent
{
        font:20px;
}

/*BankUI*/
QLabel#backgroundLabel
{
        background-image:url(:/image/image/bg.png);
}

QLabel#topbarLabel
{
        background-image:url(:/image/image/green/topbar.png);
}

QLabel#navbarLabel
{
	background-image:url(:/image/image/navbar.png);
}

QPushButton#queryBtn:pressed,
QPushButton#transferBtn:pressed,
QPushButton#paymentBtn:pressed,
QPushButton#ccardBtn:pressed
{
	background:rgba(0,0,0,30);
}

QPushButton#minBtn
{
	background-image:url(:/image/image/min0.png);
	background-repeat:no-repeat;
}

QPushButton#minBtn:hover
{
	background-image:url(:/image/image/min1.png);
	background-repeat:no-repeat;
}

QPushButton#closeBtn,
QPushButton#LogInClose,
QPushButton#MsgBoxClose
{
	background-image:url(:/image/image/close0.png);
	background-repeat:no-repeat;
}

QPushButton#closeBtn:hover,
QPushButton#LogInClose:hover,
QPushButton#MsgBoxClose:hover
{
	background-image:url(:/image/image/close1.png);
	background-repeat:no-repeat;
}

QPushButton#userBtn
{
	background-image:url(:/image/image/user0.png);
	background-repeat:no-repeat;
}

QPushButton#userBtn:hover
{
	background-image:url(:/image/image/user1.png);
	background-repeat:no-repeat;
}

QPushButton#settingBtn
{
	background-image:url(:/image/image/setting0.png);
	background-repeat:no-repeat;
}

QPushButton#settingBtn:hover
{
	background-image:url(:/image/image/setting1.png);
	background-repeat:no-repeat;
}

QLabel#queryLabel,
QLabel#transferLabel,
QLabel#paymentLabel,
QLabel#ccardLabel
{
	background:transparent;
	color:rgba(0,0,0,222);
	margin-left:80px;
}

QLabel#myAccountLabel,
QLabel#accountQueryLabel,
QLabel#currTransferLabel,
QLabel#currentFixLabel,
QLabel#ccApplyLabel,
QLabel#ccActivateLabel,
QLabel#ccRepayLabel,
QLabel#ccLostLabel
{
	background:transparent;
	color:rgba(0,0,0,222);
	font:14px;
	margin-left:104px;
}

QLabel#logInfoLabel
{
	color:rgba(0,0,0,222);
	font:14px;
}

QLabel#creditLabel
{
	color:rgba(0,0,0,222);
	font:14px;
	text-align:right;
}

QMenu#userBtnMenu,
QMenu#settingBtnMenu
{
	background:#FAFAFA;
}

QMenu#userBtnMenu::item,
QMenu#settingBtnMenu::item
{
	font:16px;
	padding:16px 24px 12px 16px;
}

QMenu#userBtnMenu::separator,
QMenu#settingBtnMenu::separator
{
	background:rgba(0,0,0,0);
	height:1px;
	margin-left:2px;
	margin-right:2px;
}

QMenu#userBtnMenu::item:selected,
QMenu#settingBtnMenu::item:selected
{
	background:#EEE;
}

/*centerStack*/
QLabel#Title
{
	background:transparent;
	border-bottom:solid;
	border-color:#DBDBDB;
	border-width:1px;
	color:rgba(0,0,0,222);
	font:32px;
}

QLabel#CMAbg
{
	background-image:url(:/image/image/CMAbg.png);
}

QLabel#CAPbg
{
        background-image:url(:/image/image/CAPbg.png);
}

QLabel#CAQbg
{
        background-image:url(:/image/image/CAQbg.png);
}

QLabel#CSMbg
{
        background-image:url(:/image/image/CSMbg.png);
}

QLabel#CMALabel2,
QLabel#CAPLabel3
{
	background:transparent;
	border-bottom:solid;
	border-color:#DBDBDB;
	border-width:1px;
	color:rgba(0,0,0,222);
	font:24px;
}

QLabel#CTTitle
{
	color:rgba(0,0,0,222);
	font:32px;
}

QLabel
{
	color:rgba(0,0,0,222);
	font:16px;
}

QPushButton#LogInReg,
QPushButton#RegBack,
QPushButton#CPAutoBtn
{
	background:transparent;
	border-radius:4px;
        color:#7CB342;
	font:bold 16px;
	padding:0 16px;
	text-align:center;
}

QPushButton#CMAPlot,
QPushButton#CAQSearch,
QPushButton#CAQRecent,
QPushButton#CAQExport,
QPushButton#CTConfirmBtn,
QPushButton#CCFConfirmBtn,
QPushButton#CPConfirmBtn,
QPushButton#CAPSettingBtn,
QPushButton#CAPCancelBtn,
QPushButton#CCAConfirmBtn,
QPushButton#CCBActivateBtn,
QPushButton#CCRConfirmBtn,
QPushButton#CCLReportBtn,
QPushButton#CCPConfirmBtn,
QPushButton#LogInLogIn,
