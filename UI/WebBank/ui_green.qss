QPushButton#MsgBoxOK,
QPushButton#CHPReturn
{
    background:#7CB342;
	border-radius:4px;
	color:#EAEAEA;
	font:bold 16px;
	padding:0 16px;
	text-align:center;
}

QPushButton#LogInReg:pressed,
QPushButton#RegBack:pressed,
QPushButton#CPAutoBtn:pressed
{
        color:#558B2F;
}

QPushButton#CMAPlot:pressed,
QPushButton#CAQSearch:pressed,
QPushButton#CAQRecent:pressed,
QPushButton#CAQExport:pressed,
QPushButton#CTConfirmBtn:pressed,
QPushButton#CCFConfirmBtn:pressed,
QPushButton#CPConfirmBtn:pressed,
QPushButton#CAPSettingBtn:pressed,
QPushButton#CAPCancelBtn:pressed,
QPushButton#CCAConfirmBtn:pressed,
QPushButton#CCBActivateBtn:pressed,
QPushButton#CCRConfirmBtn:pressed,
QPushButton#CCLReportBtn:pressed,
QPushButton#CCPConfirmBtn:pressed,
QPushButton#RegRegister:pressed,
QPushButton#LogInLogIn:pressed,
QPushButton#MsgBoxOK:pressed,
QPushButton#CHPReturn:pressed
{
        background:#689F38;
	border-bottom:solid;
        border-color:#33691E;
	border-width:2px;
	color:#EAEAEA;
