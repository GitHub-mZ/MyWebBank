QPushButton#queryBtn,
QPushButton#transferBtn,
QPushButton#paymentBtn,
QPushButton#ccardBtn,
QPushButton#myAccountBtn,
QPushButton#accountQueryBtn,
QPushButton#currTransferBtn,
QPushButton#currentFixBtn,
QPushButton#ccApplyBtn,
QPushButton#ccActivateBtn,
QPushButton#ccRepayBtn,
QPushButton#ccLostBtn,
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical
{
	background:transparent;
}

QPushButton#queryBtn:hover,
QPushButton#transferBtn:hover,
QPushButton#paymentBtn:hover,
QPushButton#ccardBtn:hover,
QPushButton#myAccountBtn:hover,
QPushButton#accountQueryBtn:hover,
QPushButton#currTransferBtn:hover,
QPushButton#currentFixBtn:hover,
QPushButton#ccApplyBtn:hover,
QPushButton#ccActivateBtn:hover,
QPushButton#ccRepayBtn:hover,
QPushButton#ccLostBtn:hover
{
	background:rgba(0,0,0,12);
}

QPushButton#queryBtn:checked,
QPushButton#transferBtn:checked,
QPushButton#paymentBtn:checked,
QPushButton#ccardBtn:checked,
QPushButton#myAccountBtn:checked,
QPushButton#accountQueryBtn:checked,
QPushButton#currTransferBtn:checked,
QPushButton#currentFixBtn:checked,
QPushButton#ccApplyBtn:checked,
QPushButton#ccActivateBtn:checked,
QPushButton#ccRepayBtn:checked,
QPushButton#ccLostBtn:checked
{
	background:rgba(0,0,0,20);
}

QComboBox
{
	background:transparent;
	border-bottom:solid;
	border-color:#DBDBDB;
	border-width:2px;
	font:16px;
}

QComboBox::drop-down
{
	background:transparent;
	color:transparent;
	width:20px;
}

QComboBox::down-arrow
{
	image:url(:/image/image/dropdown.png);
}

QComboBox QAbstractItemView::item
{
	background:#FAFAFA;
	color:rgba(0,0,0,222);
	padding:4px 12px 4px 8px;
}

QComboBox QAbstractItemView::item:selected
{
	background:#EEE;
	color:rgba(0,0,0,222);
	height:48px;
}

QLineEdit
{
	background:transparent;
	border-bottom:solid;
	border-color:#DBDBDB;
	border-width:2px;
	color:rgba(0,0,0,222);
	font:16px;
}

QLineEdit:focus
{
	background:transparent;
	border-bottom:solid;
        border-color:#7CB342;
	border-width:2px;
}

QLineEdit#CPEdit3,
QLineEdit#CCREdit2,
QLineEdit#CCREdit3,
QLineEdit#CUIEdit1,
QLineEdit#CUIEdit2,
QLineEdit#CUIEdit3,
QLineEdit#CUIEdit4,
QLineEdit#CUIEdit5,
QLineEdit#CUIEdit6,
QLineEdit#CUIEdit7,
QLineEdit#CCPEdit1,
QLineEdit#CCPEdit2,
QLineEdit#CCPEdit3,
QLineEdit#CCPEdit4
{	
	background:transparent;
	border-bottom:dotted;
	border-color:#DBDBDB;
	border-width:2px;
	color:rgba(0,0,0,222);
	font:16px;
}

QTableWidget
{
	alternate-background-color:#303337;
	background:#FFF;
	font:16px;
        selection-background-color:#DCEDC8;
	selection-color:#202020;
}

QHeaderView::section
{
	background:#FFF;
	border-bottom:solid;
	border-color:#C7C7C7;
	border-style:flat;
	border-width:1px;
	font:regular;
	height:48px;
	text-align:center;
}

QScrollBar:vertical
{
	background:transparent;
	border:3px solid transparent;
	width:12px;
}

QScrollBar::handle:vertical
{
	background:rgba(0,0,0,69);
	border-radius:3px;
	min-height:20px;
}

QScrollBar::handle:vertical:hover
{
	background:rgba(0,0,0,122);
	border-radius:3px;
	min-height:20px;
}

QCheckBox
{
	font:16px;
}

QCheckBox::indicator
{
	height:17px;
	width:17px;
}

QCheckBox::indicator:unchecked
{
	image:url(:/image/image/checkbox0.png);
}

QCheckBox::indicator:checked
{
        image:url(:/image/image/checkbox1.png);
}

QTextEdit{
	font:18px;
	background:transparent;
}

QCalendarWidget QToolButton
{
        background-color:#C5E1A5;
        height:36px;
        width:80px;
}

QCalendarWidget QSpinBox
{
        background-color:#F1F8E9;
}

QCalendarWidget QSpinBox::up-button
{
        image:url(:/image/image/spinbox-down.png);
}

QCalendarWidget QSpinBox::down-button
{
        image:url(:/image/image/spinbox-up.png);
}

QCalendarWidget QAbstractItemView:enabled
{
        background-color:#EEE;
        color:rgba(0,0,0,222);
        font-size:24px;
        selection-background-color:#7CB342;
        selection-color:rgba(0,0,0,222);
}

QLabel#calendarLabel
{
        background-color:#C5E1A5;
}
